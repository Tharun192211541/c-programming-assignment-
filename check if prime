#include<stdio.h>
int main ()
{
	int i,j,n,count;
	printf("reg.no:192211541");
	printf("\nenter the number ");
	scanf("%d",&n);
	for(i=2;i<=n;i++)
	{
		count=0;
		for(j=1;j<=i;j++)
		{
		if (i%j==0)
		{
			count++;
		}
	    }
	}
	if (count==2)
	printf(" it is prime number ");
	else
	printf(" the number is not prime");
	return 0;
}
